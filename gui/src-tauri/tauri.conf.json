{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "blitzarch-gui",
  "version": "0.1.0",
  "identifier": "com.blitzarch.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "blitzarch-gui",
        "width": 1058,
        "height": 794,
        "resizable": true,
        "fullscreen": false,
        "dragDropEnabled": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "targets": ["dmg", "appimage", "msi"],
    "active": true,
    "icon": [



      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png",
      "icons/128x128.png"
    ]
  }
}
